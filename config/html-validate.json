{
	"$schema": "https://html-validate.org/schemas/config.json",
	"extends": ["html-validate:recommended", "html-validate:standard", "html-validate:document"],
	"rules": {
		"no-dup-id": "error",
		"void-style": "off",
		"require-sri": ["error", { "target": "crossorigin" }],
		"element-required-content": "error",
		"no-trailing-whitespace": "off"
	}
}
