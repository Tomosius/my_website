{
  "ci": {
    "collect": {
      "url": ["http://localhost:4173/"],
      "numberOfRuns": 1,
      "startServerCommand": "serve -s build -l 4173"
    },
    "assert": {
      "assertions": {
        "categories:accessibility": ["warn", { "minScore": 0.95 }],
        "categories:seo": ["warn", { "minScore": 0.95 }]
      }
    },
    "upload": { "target": "temporary-public-storage" }
  }
}